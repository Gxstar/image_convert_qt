    <string notr="true">/* 主窗口背景 */
QWidget#centralwidget {
	background-color: #F0F2F5;
	font-family: &quot;Segoe UI&quot;, &quot;Microsoft YaHei&quot;;
}

/* 分组框样式 */
QGroupBox {
    font-weight: 500;
    border: 1px solid #E1E5EB;
    border-radius: 8px;
    margin-top: 1ex; /* 为标题留出空间 */
    background-color: #FAFBFD;
    font-family: &quot;Segoe UI&quot;, &quot;Microsoft YaHei&quot;;
}

QGroupBox::title {
    subcontrol-origin: margin;
    left: 10px;
    padding: 0 5px 0 5px;
    color: #5A6475;
}

/* 按钮通用样式 */
QPushButton {
    background-color: #5B8DB8;
    color: white;
    border: none;
    border-radius: 6px;
    padding: 10px 16px;
    font-weight: 500;
    font-family: &quot;Segoe UI&quot;, &quot;Microsoft YaHei&quot;;
    min-height: 24px;
}

QPushButton:hover {
    background-color: #4A7A9F;
}

QPushButton:pressed {
    background-color: #3D6785;
}

QPushButton:disabled {
    background-color: #8997A5;
}

/* 标签样式 */
QLabel {
    color: #5A6475;
    font-family: &quot;Segoe UI&quot;, &quot;Microsoft YaHei&quot;;
}

/* 组合框样式 */
QComboBox {
    padding: 8px 12px;
    border: 1px solid #D4D9E0;
    border-radius: 6px;
    background-color: #FAFBFD;
    font-family: &quot;Segoe UI&quot;, &quot;Microsoft YaHei&quot;;
    min-height: 24px;
}

QComboBox:hover {
    border-color: #5B8DB8;
}

QComboBox::drop-down {
    border: none;
    border-radius: 6px;
}

QComboBox::down-arrow {
    image: url(:/icons/down_arrow.png);
    width: 12px;
    height: 12px;
    margin-right: 8px;
}

/* 输入框样式 */
QLineEdit {
    padding: 8px 12px;
    border: 1px solid #D4D9E0;
    border-radius: 6px;
    background-color: #FAFBFD;
    font-family: &quot;Segoe UI&quot;, &quot;Microsoft YaHei&quot;;
    selection-background-color: #5B8DB8;
    selection-color: white;
    min-height: 24px;
}

QLineEdit:focus {
    border-color: #5B8DB8;
    outline: none;
}

/* 滑块样式 */
QSlider::groove:horizontal {
    height: 6px;
    background: #E1E5EB;
    border-radius: 3px;
}

QSlider::handle:horizontal {
    background: #5B8DB8;
    border: none;
    width: 18px;
    height: 18px;
    margin: -6px 0;
    border-radius: 9px;
}

QSlider::sub-page:horizontal {
    background: #8997A5;
    border-radius: 3px;
}

/* 单选按钮样式 */
QRadioButton {
    color: #5A6475;
    font-family: &quot;Segoe UI&quot;, &quot;Microsoft YaHei&quot;;
    spacing: 8px;
}

QRadioButton::indicator {
    width: 18px;
    height: 18px;
}

QRadioButton::indicator:unchecked {
    border: 2px solid #D4D9E0;
    border-radius: 9px;
    background-color: #FAFBFD;
}

QRadioButton::indicator:checked {
    border: 2px solid #5B8DB8;
    border-radius: 9px;
    background-color: #5B8DB8;
}

/* 列表视图样式 */
QListView {
    border: 1px solid #D4D9E0;
    border-radius: 6px;
    background-color: #FAFBFD;
    alternate-background-color: #F0F2F5;
    font-family: &quot;Segoe UI&quot;, &quot;Microsoft YaHei&quot;;
}

QListView::item {
    padding: 6px 8px;
}

QListView::item:selected {
    background-color: #5B8DB8;
    color: white;
}

/* LCD数字显示样式 */
QLCDNumber {
    color: #5B8DB8;
    border: none;
    background-color: transparent;
}
</string>
